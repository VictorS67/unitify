{
  "expo": {
    "name": "Unitify",
    "slug": "unitify",
    "version": "0.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.company.unitify",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAtc6gbQfdI-YdE7SoIeBXJMPmSV_LuOCk"
      },
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "App requires user location.",
        "NSLocationWhenInUseUsageDescription": "App requires user location.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "App requires location even when the App is backgrounded.",
        "UIBackgroundModes": ["remote-notification", "location", "fetch"]
      }
    },
    "android": {
      "package": "com.company.unitify",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAtc6gbQfdI-YdE7SoIeBXJMPmSV_LuOCk"
        }
      },
      "permissions": ["ACCESS_BACKGROUND_LOCATION"],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "css"]
    },
    "extra": {
      "eas": {
        "projectId": "469edaec-e6b7-4c56-b2e3-c1cfc6e602b9"
      }
    }
  }
}
